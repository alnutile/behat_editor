{
    "name": "alnutile/behat_editor",
    "description": "yml file and FeatureContext.php needed for other modules to extend it",
    "license": "GPL2.0+",
    "type": "library",
    "authors": [
        {
      "name": "Alfred Nutile",
      "email": "alfrednutile@gmail.com"
        }
    ],
    "require-dev": {
      "behat/mink-zombie-driver": "~1.0"
    }, 
    "require": {
        "behat/behat": "2.5.*@stable",
        "behat/mink": "dev-FindWindowNames as 1.5.x-dev",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium-driver": "*",
        "behat/mink-selenium2-driver": "dev-FindWindowNamesInSelenium2",
        "behat/mink-extension": "dev-CommandTimeout",
        "behat/mink-sahi-driver": "*",
        "twitter/bootstrap": "dev-master",
        "drupal/drupal-extension": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/pthurmond/Mink.git"
        },
        {
            "type": "git",
            "url": "https://github.com/pthurmond/MinkSelenium2Driver.git"
        },
        {
            "type": "git",
            "url": "https://github.com/alnutile/MinkExtension.git"
        },
        {
            "type": "git",
            "url": "http://git.drupal.org/project/drupalextension.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Drupal\\BehatEditor": "src/"
        }
    }
}
